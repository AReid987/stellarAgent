{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
       ]
    },
    "lint": {
     "dependsOn": [
       "^lint"
     ],
      "inputs": [
       "$TURBO_DEFAULT$"
     ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
   "dev": {
     "cache": false,
     "persistent": true,
      "inputs": [
       "$TURBO_DEFAULT$"
     ]
   },
    "dev:trigger": {
      "cache": false,
      "persistent": true
    },
    "commit": {
      "cache": false,
      "persistent": false
    },
    "test": {
      "cache": true,
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "test:cov": {
      "cache": true,
       "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "compile:deps": {
      "cache": true,
       "inputs": [
        "$TURBO_DEFAULT$",
        "pyproject.toml"
      ],
      "outputs": [
        "**/requirements.txt"
      ]
    },
     "install:deps": {
      "cache": true,
       "inputs": [
        "**/requirements.txt"
       ],
      "outputs": [
        "**/.venv/**"
      ]
    },
    "lint:fix": {
       "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "clean": {
       "inputs": [
        "$TURBO_DEFAULT$"
      ]
    }
  },
  "globalDependencies": [
    ".python-version"
  ],
  "globalEnv": [
    "PYTHONPATH",
    "FASTAPI_ENV"
  ]
}
